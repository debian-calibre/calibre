msgstr ""

#: /__w/calibre/calibre/src/calibre/devices/kobo/kobotouch_config.py:894
msgid "Pages:"
msgstr "पाने:"

#: /__w/calibre/calibre/src/calibre/devices/kobo/kobotouch_config.py:895
