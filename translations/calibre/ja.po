msgstr "ハーフ&ハーフ"

#: /__w/calibre/calibre/src/calibre/ebooks/covers.py:411
msgid "Banner"
msgstr "バナー"

#: /__w/calibre/calibre/src/calibre/ebooks/covers.py:481
msgid "Ornamental"
msgstr "装飾枠"

#: /__w/calibre/calibre/src/calibre/ebooks/covers.py:548
msgid "Blocks"
msgstr "ブロック"

