msgstr "ヘッダにIDをつける"

#: /home/kovid/work/calibre/src/calibre/ebooks/conversion/plugins/txt_input.py:21
msgid "Metadata in the document"
msgstr "ドキュメントの書誌情報"

#: /home/kovid/work/calibre/src/calibre/ebooks/conversion/plugins/txt_input.py:22
msgid "Support tables"
msgstr "表組み(Tables)のサポート"

#: /home/kovid/work/calibre/src/calibre/ebooks/conversion/plugins/txt_input.py:23
