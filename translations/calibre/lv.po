msgstr "Pievienot atribūtu HTML birkām"

#: /home/runner/work/calibre/calibre/src/calibre/ebooks/conversion/plugins/txt_input.py:18
msgid "Add code highlighting via Pygments"
msgstr "Pievienot koda izcelšanu ar Pygments"

#: /home/runner/work/calibre/calibre/src/calibre/ebooks/conversion/plugins/txt_input.py:19
msgid "Definition lists"
msgstr "Definīciju saraksti"

#: /home/runner/work/calibre/calibre/src/calibre/ebooks/conversion/plugins/txt_input.py:20
msgid "Enables various common extensions"
msgstr "Iespējo dažādus izplatītus paplašinājumus"

#: /home/runner/work/calibre/calibre/src/calibre/ebooks/conversion/plugins/txt_input.py:21
msgid "Alternative code block syntax"
msgstr "Alternatīvā koda bloka sintakse"

