msgid "Minimum word length to hyphenate, in characters."
msgstr ""

#: /__w/calibre/calibre/src/calibre/ebooks/conversion/plugins/epub_output.py:639
msgid "Minimum characters before hyphens."
msgstr ""

#: /__w/calibre/calibre/src/calibre/ebooks/conversion/plugins/epub_output.py:643
msgid "Minimum characters after hyphens."
msgstr ""

