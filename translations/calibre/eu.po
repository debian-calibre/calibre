msgstr "strcat(a, b, ...) -- edozein argumentu kopuru onartuko du. Argumentu guztiak elkarri lotzen dioten katea emango du"

#: /home/kovid/work/calibre/src/calibre/utils/formatter_functions.py:219
msgid "strlen(a) -- Returns the length of the string passed as the argument"
msgstr "strlen(a) -- argumentu legez igaro den katearen luzeera emango du"

#: /home/kovid/work/calibre/src/calibre/utils/formatter_functions.py:232
msgid ""
"add(x, y) -- returns x + y. Throws an exception if either x or y are not "
"numbers."
msgstr "add(x, y) -- emango: x + y. Errore emango du baldin x edo y ez badira biak zenbaki."

#: /home/kovid/work/calibre/src/calibre/utils/formatter_functions.py:243
msgid ""
"subtract(x, y) -- returns x - y. Throws an exception if either x or y are "
"not numbers."
msgstr "subtract(x, y) -- emango: x - y. Errore emango du baldin x edo y ez badira biak zenbaki."

#: /home/kovid/work/calibre/src/calibre/utils/formatter_functions.py:254
msgid ""
"multiply(x, y) -- returns x * y. Throws an exception if either x or y are "
