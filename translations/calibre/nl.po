#: /__w/calibre/calibre/src/calibre/ebooks/oeb/polish/check/links.py:348
#, python-format
msgid "The linked resource %s does not exist"
msgstr "De gelinkte bron %s bestaat niet"

#: /__w/calibre/calibre/src/calibre/ebooks/oeb/polish/check/links.py:353
msgid "The URL {} could not be parsed"
msgstr "De URL {} kan niet worden geparseerd"

#: /__w/calibre/calibre/src/calibre/ebooks/oeb/polish/check/links.py:356
#, python-format
msgid "The link %s is a file:// URL"
msgstr "De link %s is een file:// URL"

#: /__w/calibre/calibre/src/calibre/ebooks/oeb/polish/check/links.py:358
#, python-format
msgid "The link %s points to a file outside the book"
msgstr "De link %s verwijst naar een bestand buiten het boek"

#: /__w/calibre/calibre/src/calibre/ebooks/oeb/polish/check/links.py:361
#, python-format
msgid ""
"The link %s contains a : character, this will cause errors on Windows "
"computers"
msgstr "De verwijzing %s bevat een : teken, dit zal tot fouten leiden op Windows-computers"

#: /__w/calibre/calibre/src/calibre/ebooks/oeb/polish/check/opf.py:20
