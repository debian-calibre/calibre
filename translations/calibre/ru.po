#: /__w/calibre/calibre/src/calibre/gui2/tweak_book/reports.py:542
msgid "Source"
msgstr "Источник"

#: /__w/calibre/calibre/src/calibre/gui2/preferences/metadata_sources.py:54
msgid "Cover priority"
msgstr "Приоритет обложки"

#: /__w/calibre/calibre/src/calibre/gui2/preferences/metadata_sources.py:81
msgid "This source is configured and ready to go"
msgstr "Этот источник настроен и готов к использованию"

