msgstr "není"

#: /home/kovid/work/calibre/src/calibre/ebooks/css_transform_rules.py:229
msgid "is any value"
msgstr "je jakákoliv hodnota"

#: /home/kovid/work/calibre/src/calibre/ebooks/css_transform_rules.py:230
#: /home/kovid/work/calibre/src/calibre/gui2/preferences/coloring.py:83
