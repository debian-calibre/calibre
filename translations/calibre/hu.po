#: /__w/calibre/calibre/src/calibre/srv/opds.py:193
#, python-format
msgid "SERIES: %(series)s [%(sidx)s]<br />"
msgstr "SOROZAT: %(series)s [%(sidx)s]<br />"

#: /__w/calibre/calibre/src/calibre/srv/opds.py:290
msgid "Books in your library"
msgstr "A könyvtárában lévő könyvek"

#: /__w/calibre/calibre/src/calibre/srv/opds.py:295
msgid "By "
msgstr "– "

#: /__w/calibre/calibre/src/calibre/srv/opds.py:296
msgid "Books sorted by "
msgstr "Könyvek rendezve "

#: /__w/calibre/calibre/src/calibre/srv/opds.py:303
msgid "Library:"
msgstr "Könyvtár:"

#: /__w/calibre/calibre/src/calibre/srv/opds.py:306
msgid "Change calibre library to:"
msgstr "Calibre könyvtár váltása:"

#: /__w/calibre/calibre/src/calibre/srv/opds.py:417
#: /__w/calibre/calibre/src/calibre/srv/opds.py:434
