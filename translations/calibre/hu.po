#: /__w/calibre/calibre/src/calibre/devices/usbms/deviceconfig.py:60
msgid "settings for device drivers"
msgstr "eszközmeghajtók beállításai"

#: /__w/calibre/calibre/src/calibre/devices/usbms/deviceconfig.py:62
msgid "Ordered list of formats the device will accept"
