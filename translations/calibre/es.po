#, python-format
msgid "idref=\"%s\" points to unknown id"
msgstr "idref=\"%s\" apunta a un id desconocido"

#: /home/kovid/work/calibre/src/calibre/ebooks/oeb/polish/check/opf.py:31
#, python-format
