#: /home/kovid/work/calibre/src/calibre/ebooks/oeb/polish/check/links.py:344
#, python-format
msgid "The linked resource %s does not exist"
msgstr "El recurso enlazado %s no existe"

#: /home/kovid/work/calibre/src/calibre/ebooks/oeb/polish/check/links.py:348
#, python-format
msgid "The link %s is a file:// URL"
msgstr "El enlace %s es un URL de tipo file://"

#: /home/kovid/work/calibre/src/calibre/ebooks/oeb/polish/check/links.py:350
#, python-format
msgid "The link %s points to a file outside the book"
msgstr "El enlace %s apunta a un archivo externo al libro"

#: /home/kovid/work/calibre/src/calibre/ebooks/oeb/polish/check/links.py:352
