#: /__w/calibre/calibre/src/calibre/devices/smart_device_app/driver.py:1703
#, python-format
msgid "The read date sync column %s is not in calibre's library"
msgstr "Kolumny daty %s nie ma w bibliotece"

#: /__w/calibre/calibre/src/calibre/devices/smart_device_app/driver.py:1708
#, python-format
msgid "The read date sync column %s is not a date column"
msgstr "Kolumna synchronizacji daty %s nie jest kolumną typu Data"

#: /__w/calibre/calibre/src/calibre/devices/smart_device_app/driver.py:1907
#, python-format
msgid ""
"Bad compression quality setting. It must be a number between 50 and 99. "
"Forced to be %d."
msgstr "Niewłaściwe ustawienie stopnia kompresji. Wartość musi zawierać się między 50 a 99. Ustawiono na %d."

