msgid "Error: Action %s not recognized, must be one of: (add|remove|list)"
msgstr "Virhe: Komentoa %s ei tunnisteta, käytettävissä ovat: [addlremovellist}"

#: /__w/calibre/calibre/src/calibre/db/cli/cmd_search.py:16
#, python-brace-format
