#: /home/kovid/work/calibre/src/calibre/devices/smart_device_app/driver.py:1679
#, python-format
msgid "The read date sync column %s is not in calibre's library"
msgstr "La columna de sincronització de la data de lectura %s no està a la biblioteca de calibre"

#: /home/kovid/work/calibre/src/calibre/devices/smart_device_app/driver.py:1684
