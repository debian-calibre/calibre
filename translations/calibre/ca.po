msgid "Half and Half"
msgstr "Mig a mig"

#: /home/kovid/work/calibre/src/calibre/ebooks/covers.py:382
msgid "Banner"
msgstr "Banner"

#: /home/kovid/work/calibre/src/calibre/ebooks/covers.py:451
msgid "Ornamental"
msgstr ""

