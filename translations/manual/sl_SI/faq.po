msgstr ""

# c0217f710aae4f29b7f4f409c23a37ef
#: ../../home/kovid/work/calibre/manual/faq.rst:733
msgid "Why the name calibre?"
msgstr ""

# e1078687aac94c79b65c0d3f7218b11d
#: ../../home/kovid/work/calibre/manual/faq.rst:738
msgid "Take your pick:"
msgstr ""

# d0e56f4458214fbf814e020b8177fa6b
#: ../../home/kovid/work/calibre/manual/faq.rst:735
msgid "Convertor And LIBRary for Ebooks"
msgstr ""

# 817dad116ce2407cbd35bf1826f1635d
#: ../../home/kovid/work/calibre/manual/faq.rst:736
msgid "A high *calibre* product"
msgstr ""

# 54f5d6b05b07460bbb0fcd5d60164f00
